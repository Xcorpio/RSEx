{
	"name": "RSEx",
	"version": "1.0",
	"manifest_version": 2,

	"description": "RS辅助扩展",
	"icons":{
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},

	"browser_action":{
		"default_icon": "icons/icon19.png",
		"default_popup": "popup.html",
		"default_title": "RSEx"
	},
  
	"permissions":["*://rs.xidian.edu.cn/","cookies","tabs","storage","alarms"]
}